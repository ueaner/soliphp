{
    "name": "soliphp/soliphp",
    "description": "The Soli PHP Framework.",
    "keywords": ["framework", "soliphp", "soli"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "soliphp/framework": "^1.3.3",
        "soliphp/session": "^1.0",
        "soliphp/view": "^1.0",
        "soliphp/db": "^1.0",
        "monolog/monolog": "^1.23",
        "twig/twig": "^2.4"
    },
    "require-dev": {
        "filp/whoops": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
