{
    "name": "soliphp/soliphp",
    "description": "The Soli PHP Framework.",
    "keywords": ["framework", "soliphp", "soli"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.0",
        "soliphp/framework": "dev-master",
        "soliphp/db": "^1.0",
        "monolog/monolog": "^1.23",
        "twig/twig": "^2.4"
    },
    "require-dev": {
        "filp/whoops": "~2.1",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
