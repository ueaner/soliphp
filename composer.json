{
    "name": "soliphp/soliphp",
    "description": "The Soli PHP Framework.",
    "keywords": ["framework", "soliphp", "soli"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "soliphp/framework": "^2.0",
        "soliphp/web": "^0.5",
        "soliphp/console": "^1.2",
        "soliphp/view": "^1.1",
        "soliphp/db": "^1.2",
        "monolog/monolog": "^1.24",
        "twig/twig": "^2.7"
    },
    "require-dev": {
        "filp/whoops": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
